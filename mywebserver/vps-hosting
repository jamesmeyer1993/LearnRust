    html[data-useragent*='MSIE'] body {}
    html[data-useragent*='MSIE'] .browser-alert { position: fixed; top: 0; left: 0; right: 0; z-index: 999999; background-color: #fcf8e3; }
    html[data-useragent*='MSIE'] .browser-alert p { margin-bottom: 5px; }
    html[data-useragent*='MSIE'] .browser-alert .alert .close { top: -4px; right: 0; }
    html[data-useragent*='MSIE'] .browser-alert .alert { margin: 0 auto; border: none; padding-bottom: 2px; text-align: center; width: 73.076em; width: 71.250em; min-width: 950px; }
</style>

<div class="browser-alert hidden">
    <div class="alert" data-alert="alert">
        <button type="button" class="close" data-dismiss="alert" onclick="browserAlert.hideMe()">x</button>
        <p class="ie-browserhappy">
            The browser version you are using is <strong>no longer supported</strong>. Please <a href="http://browsehappy.com/" target="_blank" rel="noopener">upgrade</a> for the best site experience.
        </p>
    </div>
</div>

<script type="text/javascript">

    browserAlert = new Object();

    browserAlert.runOnLoad = function() {

        if (Cookies.get('alert-box') == null) {
            Cookies.set('alert-box', 'open', { expires: 7, path: '/' });
            browserAlert.addBodyStyle();
        } else if(Cookies.get('alert-box') == 'close') {
            $(".browser-alert").hide();
            browserAlert.removeBodyStyle();
        }
    }

    browserAlert.hideMe = function() {
        Cookies.set('alert-box', 'close', {expires:7, path: '/' });
        $(".browser-alert").hide();
        browserAlert.removeBodyStyle();
    }

    browserAlert.addBodyStyle = function() {
        var doc = document.documentElement;
        doc.setAttribute('data-useragent', navigator.userAgent);

        var ieBAlert = $('html[data-useragent*="MSIE"] .browser-alert');
        ieBAlert.prependTo('body').removeClass('hidden');
        $('body').css({ 'top': ieBAlert.height(), 'position': 'relative' });
    }

    browserAlert.removeBodyStyle = function() {
        $('body').removeAttr('style');
    }

    browserAlert.runOnLoad();

</script>

<!-- End tag. -->



