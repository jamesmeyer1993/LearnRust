<script type="text/javascript">

document.addEventListener('DOMContentLoaded', function() {
  var container = document.getElementById('vps-compare-chart');
  var features = container.getElementsByClassName('feature');

  for (var i = 0; i < features.length; i++) {
    var hasCaret = features[i].getElementsByClassName('caret')

    if (hasCaret[0]) {
      features[i].addEventListener('click', function() {
        var caret = this.getElementsByClassName('caret');
        var $ul = $(this).find('ul');

        container.classList.toggle('open');
        this.classList.toggle('open');
        caret[0].classList.toggle('open');
        $ul.slideToggle()
      });
    }
  }
});

</script>

